{
  "buildCommand": "npx expo export --platform web && cp privacy-policy.html dist/privacy.html",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    { "source": "/privacy", "destination": "/privacy.html" },
    { "source": "/privacy/", "destination": "/privacy.html" },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
